				{
					"name": "Refresh",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse JSON safely and set environment tokens and expirations",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"let json;",
									"try {",
									"    json = pm.response.json();",
									"} catch (e) {",
									"    json = {};",
									"}",
									"",
									"pm.test(\"Response has access and refresh keys\", function () {",
									"    pm.expect(json).to.be.an(\"object\");",
									"    pm.expect(json).to.have.property(\"access\");",
									"    pm.expect(json).to.have.property(\"refresh\");",
									"});",
									"",
									"if (json && typeof json === 'object') {",
									"    if (json.access) {",
									"        pm.environment.set('access_token', json.access);",
									"    }",
									"    if (json.refresh) {",
									"        pm.environment.set('refresh_token', json.refresh);",
									"    }",
									"    if (json.access_expiration) {",
									"        pm.environment.set('access_expiration', String(json.access_expiration));",
									"    }",
									"    if (json.refresh_expiration) {",
									"        pm.environment.set('refresh_expiration', String(json.refresh_expiration));",
									"    }",
									"}",
									"",
									"// Optional logging for visibility in the Postman console",
									"console.log('Tokens and expirations updated in environment:', {",
									"    access_token: pm.environment.get('access_token') ? '[set]' : '[missing]',",
									"    refresh_token: pm.environment.get('refresh_token') ? '[set]' : '[missing]',",
									"    access_expiration: pm.environment.get('access_expiration') || '[none]',",
									"    refresh_expiration: pm.environment.get('refresh_expiration') || '[none]'",
									"});"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						},
